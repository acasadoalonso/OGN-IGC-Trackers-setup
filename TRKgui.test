import customtkinter as ctk
def execute():
    print ("executing")
ctk.set_appearance_mode('dark')
ctk.set_default_color_theme('dark-blue')
root = ctk.CTk()
frame= ctk.CTkFrame(master=root)
frame.pack(pady=20, padx=60, fill="both", expand=True)
root.geometry("2500x1500")
root.title("TRKsetup")
label = ctk.CTkLabel(frame, text="Trackers Setup", font=('Arial', 20))
label.pack(padx=20, pady=20)
textbox = ctk.CTkTextbox(frame, height=1, font=('Arial', 16))
textbox.pack()
myentry1 = ctk.CTkEntry(master=frame,placeholder_text="E1")
myentry1.pack(pady=12, padx=10)
myentry2 = ctk.CTkEntry(master=frame,placeholder_text="E2")
myentry2.pack(pady=12, padx=10)
buttom   = ctk.CTkButton(master=frame, text="execute", command=execute)
buttom.pack(pady=12, padx=10)
checkbox1 = ctk.CTkCheckBox(master=frame, text="opt1")
checkbox1.pack(pady=12,padx=10)
root.mainloop()
